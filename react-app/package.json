{
  "name": "playmakers-react",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3005 --strictPort",
    "dev:force": "PIDS=$(lsof -ti tcp:3005 || true); if [ -n \"$PIDS\" ]; then echo \"Killing port 3005 PIDs: $PIDS\"; echo \"$PIDS\" | xargs kill -9 || true; fi; vite --port 3005 --strictPort",
    "build": "vite build",
    "preview": "vite preview --port 3006 --strictPort"
  },
  "dependencies": {
    "puppeteer": "^24.31.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^7.2.2"
  }
}
